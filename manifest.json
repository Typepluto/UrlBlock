{
  "name": "UrlBlock",
  "description": "Go study!Stop wasting your time on some url!",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "./32x32.png",
    "32": "./32x32.png",
    "48": "./32x32.png",
    "128": "./32x32.png"
  },
  "permissions": ["tabs"],
  "host_permissions": ["http://*/*", "https://*/*"]
}